{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "productA",
    "productB",
    "comparisons"
  ],
  "properties": {
    "productA": {
      "type": "object"
    },
    "productB": {
      "type": "object"
    },
    "comparisons": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "metric",
          "productA",
          "productB",
          "notes"
        ]
      }
    }
  }
}